{
  "schema": "https://github.com/awslabs/frontend-discovery/blob/main/schema/v1-pre.json",
  "microFrontends": {
    "HomeMFE": [
      {
        "url": "http://localhost:2001/remoteEntry.js",
        "metadata": {
          "integrity": "sha256-initial",
          "version": "1.0.0"
        },
        "extras": {
          "name": "HomeMFE",
          "alias": "HomeMFE",
          "exposed": "MFE",
          "route": "/"
        }
      }
    ],
    "CatalogueMFE": [
      {
        "url": "http://localhost:2002/remoteEntry.js",
        "metadata": {
          "integrity": "sha256-initial",
          "version": "1.0.0"
        },
        "extras": {
          "name": "CatalogueMFE",
          "alias": "CatalogueMFE",
          "exposed": "MFE",
          "route": "/catalogue/*"
        }
      }
    ],
    "MyAccountMFE": [
      {
        "url": "http://localhost:2003/remoteEntry.js",
        "metadata": {
          "integrity": "sha256-initial",
          "version": "1.0.0"
        },
        "extras": {
          "name": "myAccountMFE",
          "alias": "MyAccountMFE",
          "exposed": "MFE",
          "route": "/my-account/*"
        }
      }
    ],
    
    "UserPaymentMethodsMFE": [
      {
        "url": "http://localhost:2005/remoteEntry.js",
        "metadata": {
          "integrity": "sha256-initial",
          "version": "1.0.0"
        },
        "extras": {
          "name": "UserPaymentMethodsMFE",
          "alias": "UserPaymentMethodsMFE",
          "exposed": "MFE"
        }
      }
    ]
  }
} 